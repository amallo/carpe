# Configuration des providers
# true = utiliser les providers simulés (développement)
# false = utiliser les providers réels (production)
USE_MOCK_PROVIDERS=true

# Configuration des logs de débogage
# true = activer les logs de débogage
# false = désactiver les logs de débogage
ENABLE_DEBUG_LOGS=true

# URL de l'API
API_BASE_URL=https://api.carpeapp.com 